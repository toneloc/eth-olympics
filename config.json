{
  "testMatch": ["<rootDir>/src/**/*.test.js"],
  "setupFiles": ["<rootDir>/scripts/test/setup.js"],
  "transform": {".*": "<rootDir>/node_modules/babel-jest"},
  "moduleFileExtensions": ["js"],
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "moduleDirectories": ["node_modules", "src", "scripts/test"]
}
